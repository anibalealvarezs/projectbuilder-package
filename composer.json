{
    "name": "anibalealvarezs/projectbuilder-package",
    "description": "Package to add usefull libraries for new projects",
    "keywords": [
        "project",
        "laravel",
        "builder",
        "productivity",
        "crud",
        "api"
    ],
    "homepage": "https://anibalalvarez.com",
    "license": "MIT",
    "version" : "1.3.7",
    "type" : "package",
    "authors": [
        {
            "name": "Aníbal Álvarez",
            "email": "anibalealvarezs@gmail.com",
            "homepage": "https://anibalalvarez.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-permission": "^5.0",
        "spatie/laravel-translatable": "^5.0",
        "barryvdh/laravel-debugbar": "^3.6.5"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Anibalealvarezs\\Projectbuilder\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbAppServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbCommandServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbComposerServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbConfigServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbControllerServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbMiddlewareServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbMigrationServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbViewServiceProvider",
                "Anibalealvarezs\\Projectbuilder\\Providers\\PbSeederServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
