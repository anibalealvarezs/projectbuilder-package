{
    "name": "anibalealvarezs/projectbuilder-package",
    "description": "Package to add usefull libraries for new projects",
    "keywords": [
        "project",
        "laravel",
        "builder",
        "productivity"
    ],
    "homepage": "https://anibalalvarez.com",
    "license": "MIT",
    "version" : "1.0.5",
    "type" : "package",
    "authors": [
        {
            "name": "Aníbal Álvarez",
            "email": "anibalealvarezs@gmail.com",
            "homepage": "https://anibalalvarez.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.3",
        "laravel/framework": "^8.0",
        "spatie/laravel-permission": "^3.17"
    },
    "autoload": {
        "psr-4": {
            "Anibalealvarezs\\Projectbuilder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anibalealvarezs\\Projectbuilder\\ProjectbuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}